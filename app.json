{
  "expo": {
    "name": "BookNote",
    "slug": "book-note",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bjakubov.booknote"
    },
    "githubUrl": "https://github.com/b-jakubowski/BookNote",
    "sdkVersion": "37.0.0",
    "android": {
      "package": "com.bjakubov.booknote"
    }
  }
}